function get_scene_title(argument0) {
	switch argument0
	{
		case loc.hotel: return "My Hotel Room"; break;
		case loc.pstation: return "Police Station"; break;
		case loc.cafe: return "Nearby Cafe"; break;
		case loc.market: return "The Market"; break;
		case loc.alley: return "Shady Alley"; break;
		case loc.park: return "Park"; break;
		case loc.void: return "???"; break;
	}


}
